<%= form_with(model: alliance_membership_request, local: true) do |form| %>
  <%= render "shared/errors", object: alliance_membership_request %>


  <div class="field">
    <%= form.label :user_id %>
    <%= form.text_field :user_id %>
  </div>

  <div class="field">
    <%= form.label :alliance_id %>
    <%= form.text_field :alliance_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
